package mx.ine.computosINE.dto.helper;

import java.io.Serializable;

 /**
 * Objeto de Transferencia de Datos que contiene los valores del identificador
 * y nombre del entorno geográfico
 * 
 * @author José Antonio López Torres
 * @since 12/05/2017
 * @copyright Direccion de sistemas - INE
 */
public class HLPEntornoGeografico implements Serializable{

    /**
     * Elemento para la serialización de los objetos generados por esta clase
     */
    private static final long serialVersionUID = 4557722563013599569L;
    
    /**
     * Identidicador geográfico
     */
    private Integer id;
    /**
     * Nombre del entorno geográfico
     */
    private String nombre;
    /**
     * Propiedad para validar si el entorno ya cuenta con recuento
     * True-con recuento, False-sin recuento
     */
    private boolean conRecuento;
    
    /**
     * Método que obtiene el valor de el atributo id
     *
     * @return Integer : valor que tiene el atributo id
     * 
     * @author José Antonio López Torres
     * @since 12/05/2017
     */
    public Integer getId() {
        return id;
    }
    /**
     * Método que ingresa el valor de el atributo id
     *
     * @param id : valor que ingresa a el atributo id
     *
     * @author José Antonio López Torres
     * @since 12/05/2017
     */
    public void setId(Integer id) {
        this.id = id;
    }
    /**
     * Método que obtiene el valor de el atributo nombre
     *
     * @return String : valor que tiene el atributo nombre
     * 
     * @author José Antonio López Torres
     * @since 12/05/2017
     */
    public String getNombre() {
        return nombre;
    }
    /**
     * Método que ingresa el valor de el atributo nombre
     *
     * @param nombre : valor que ingresa a el atributo nombre
     *
     * @author José Antonio López Torres
     * @since 12/05/2017
     */
    public void setNombre(String nombre) {
        this.nombre = nombre;
    }
    /**
     * Método que obtiene el valor de el atributo conRecuento
     *
     * @return boolean : valor que tiene el atributo conRecuento
     * 
     * @author José Antonio López Torres
     * @since 05/06/2017
     */
    public boolean getConRecuento() {
        return conRecuento;
    }
    /**
     * Método que ingresa el valor de el atributo conRecuento
     *
     * @param conRecuento : valor que ingresa a el atributo conRecuento
     *
     * @author José Antonio López Torres
     * @since 05/06/2017
     */
    public void setConRecuento(boolean conRecuento) {
        this.conRecuento = conRecuento;
    }
}
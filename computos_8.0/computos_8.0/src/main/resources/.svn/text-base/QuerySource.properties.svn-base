query_recuento_total_geografia		= SELECT DISTINCT A.ID FROM (SELECT <!REM_SELECT> FROM ( SELECT A.ID_ESTADO, <!REM_GENERAL> ((A.VOTOS_PRIMERO - A.VOTOS_SEGUNDO)/A.TOTAL)*100 PORCENTAJE FROM ( SELECT A.ID_ESTADO, <!REM_GENERAL> A.VOTOS_PRIMERO, B.VOTOS_SEGUNDO, C.TOTAL FROM ( SELECT DISTINCT A.ID_ESTADO, <!REM_GENERAL> A.VOTOS VOTOS_PRIMERO, A.PUESTO PRIMERO FROM ( SELECT DISTINCT A.ID_ESTADO, <!REM_GENERAL> A.ID_ASOCIACION||TIPO_ASOCIACION ASOCIACION, A.VOTOS, ROW_NUMBER () OVER (PARTITION BY A.ID_ESTADO <!REM_ORDER_GROUP> ORDER BY A.VOTOS DESC) AS PUESTO FROM COMPUTOSINE.DISTRIBUCION_CANDIDATOS A WHERE A.ID_PROCESO_ELECTORAL=:PROCESO AND A.ID_DETALLE_PROCESO=:DETALLE AND A.ID_ESTADO=:ESTADO AND A.ID_TIPO_CANDIDATURA=:CANDIDATURA AND A.ID_ASOCIACION > 0 <!REM_WHERE> )A WHERE A.PUESTO = 1 ORDER BY A.ID_ESTADO, <!REM_GENERAL> A.PUESTO )A INNER JOIN (SELECT DISTINCT A.ID_ESTADO, <!REM_GENERAL> A.VOTOS VOTOS_SEGUNDO, A.PUESTO SEGUNDO FROM ( SELECT DISTINCT A.ID_ESTADO, <!REM_GENERAL> A.ID_ASOCIACION||TIPO_ASOCIACION ASOCIACION, A.VOTOS, ROW_NUMBER () OVER (PARTITION BY A.ID_ESTADO <!REM_ORDER_GROUP> ORDER BY A.VOTOS DESC) AS PUESTO FROM COMPUTOSINE.DISTRIBUCION_CANDIDATOS A WHERE A.ID_PROCESO_ELECTORAL=:PROCESO AND A.ID_DETALLE_PROCESO=:DETALLE AND A.ID_ESTADO=:ESTADO AND A.ID_TIPO_CANDIDATURA=:CANDIDATURA AND A.ID_ASOCIACION > 0 <!REM_WHERE> )A WHERE A.PUESTO = 2 ORDER BY A.ID_ESTADO, <!REM_GENERAL> A.PUESTO )B ON A.ID_ESTADO=B.ID_ESTADO <!REM_JOIN_AB> LEFT JOIN (SELECT DISTINCT A.ID_ESTADO, <!REM_GENERAL> A.TOTAL FROM ( SELECT DISTINCT A.ID_ESTADO, <!REM_GENERAL> SUM(A.VOTOS) TOTAL FROM COMPUTOSINE.DISTRIBUCION_CANDIDATOS A WHERE A.ID_PROCESO_ELECTORAL=:PROCESO AND A.ID_DETALLE_PROCESO=:DETALLE AND A.ID_ESTADO=:ESTADO AND A.ID_TIPO_CANDIDATURA=:CANDIDATURA AND A.ID_ASOCIACION > 0 <!REM_WHERE> GROUP BY A.ID_ESTADO <!REM_ORDER_GROUP> )A ORDER BY A.ID_ESTADO <!REM_ORDER_GROUP> )C ON A.ID_ESTADO=C.ID_ESTADO <!REM_JOIN_AC> ORDER BY A.ID_ESTADO <!REM_ORDER_GROUP> )A )A WHERE A.PORCENTAJE <= 1 UNION ALL SELECT <!REM_SELECT> FROM (SELECT A.ID_ESTADO, <!REM_GENERAL> ROUND((NUM_PARCIAL * 100)/NUM_TOTAL,5) PORCENTAJE FROM ( SELECT DISTINCT A.ID_ESTADO, <!REM_GENERAL> COUNT(A.SECCION||A.ID_CASILLA||A.TIPO_CASILLA||A.EXT_CONTIGUA) NUM_TOTAL FROM COMPUTOSINE.ACTA_CASILLA_VOTOS A WHERE A.ID_PROCESO_ELECTORAL=:PROCESO AND A.ID_DETALLE_PROCESO=:DETALLE AND A.ID_ESTADO=:ESTADO AND A.ID_TIPO_CANDIDATURA=:CANDIDATURA AND A.ID_ESTATUS != 5 <!REM_WHERE> GROUP BY A.ID_ESTADO <!REM_ORDER_GROUP> ORDER BY A.ID_ESTADO <!REM_ORDER_GROUP> )A RIGHT JOIN ( SELECT DISTINCT A.ID_ESTADO, <!REM_GENERAL> COUNT(A.SECCION||A.ID_CASILLA||A.TIPO_CASILLA||A.EXT_CONTIGUA) NUM_PARCIAL FROM COMPUTOSINE.ACTA_CASILLA_VOTOS A WHERE A.ID_PROCESO_ELECTORAL=:PROCESO AND A.ID_DETALLE_PROCESO=:DETALLE AND A.ID_ESTADO=:ESTADO AND A.ID_TIPO_CANDIDATURA=:CANDIDATURA AND A.ID_ESTATUS=2 AND A.ID_ESTATUS != 5 <!REM_WHERE> GROUP BY A.ID_ESTADO <!REM_ORDER_GROUP> ORDER BY A.ID_ESTADO <!REM_ORDER_GROUP> )B ON A.ID_ESTADO = B.ID_ESTADO <!REM_JOIN_AB> )A WHERE A.PORCENTAJE >= 20)A ORDER BY ID

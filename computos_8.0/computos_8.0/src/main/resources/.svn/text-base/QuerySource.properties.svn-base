query_reportes_inicializaEncabezado_concentrado_por_estado                             =   SELECT DISTINCT dp.ID_ASOCIACION, \
 dp.TIPO_ASOCIACION, \
 asoc.SIGLAS, \
 dp.EMBLEMA, \
 dp.ORDEN \
FROM COMPUTOSINE.DISTRIBUCION_PARTIDOS_CI dp \
LEFT JOIN ORGANOS_DIRECTIVOS.ASOCIACIONES asoc \
ON dp.ID_ASOCIACION           = asoc.ID_ASOCIACION \
AND dp.ID_ESTADO              = asoc.ID_ESTADO \
AND dp.TIPO_ASOCIACION        = asoc.TIPO_ASOCIACION \
WHERE dp.ID_PROCESO_ELECTORAL = :P_ID_PROCESO_ELECTORAL \
AND dp.ID_DETALLE_PROCESO     =:P_ID_DETALLE_PROCESO \
AND dp.ID_ESTADO              =:P_ID_ESTADO \
AND dp.ID_TIPO_CANDIDATURA   IN (7,8) \
AND dp.TIPO_ASOCIACION      = 1 \
AND dp.ID_DISTRITO          <>   -5\
ORDER BY dp.TIPO_ASOCIACION, \
 dp.ORDEN ASC


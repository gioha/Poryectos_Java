#---------------------------------------------------------------------------------
# 							            QUERYS
#
# Contiene los querys que se utilizaran en cada modulo o de manera general
# 
# Nomenclatura query: query_"modulo al que pertenece"_"nombre del query"
#
# Ejemplo: query_administracionUsuarios_todo = Select * FROM usuarios.
#---------------------------------------------------------------------------------


query_admonIndicadoresAnio_indicadoresDisponiblesRecurrentes = SELECT * FROM (SELECT row_.*, rownum rownum_ FROM (SELECT idIndicador, idProceso, idSubProceso, "nombre" as nombre, "proceso.etiqueta" as procesoEtiqueta, "subProceso.etiqueta" as subProcesoEtiqueta, "nivelPrivacidad.etiqueta" as nivelPrivacidadEtiqueta, habilitado FROM ( SELECT idIndicador, idProceso, idSubProceso, nombre as "nombre", procesoEtiqueta as "proceso.etiqueta", subProcesoEtiqueta as "subProceso.etiqueta", nivelPrivacidadEtiqueta as "nivelPrivacidad.etiqueta", habilitado FROM ( SELECT COUNT(ANIO) AS TOTAL, INA.ID_INDICADOR AS idIndicador, INA.PROCESO AS idProceso, INA.SUBPROCESO AS idSubProceso, IND.NOMBRE, PRO.ETIQUETA AS procesoEtiqueta, SUB.ETIQUETA AS subProcesoEtiqueta, NP.ETIQUETA AS nivelPrivacidadEtiqueta, ind.habilitado as habilitado FROM PROCESOS PRO, C_NIVELES_PRIVACIDAD NP, INDICADORES IND, INDICADORES_ANIO INA LEFT OUTER JOIN SUBPROCESOS SUB ON INA.SUBPROCESO = SUB.ID_SUBPROCESO AND INA.PROCESO = SUB.ID_PROCESO WHERE ANIO <> :anioConfiguracion AND INA.PROCESO = PRO.ID_PROCESO AND IND.ID_INDICADOR = INA.ID_INDICADOR AND IND.NIVEL_PRIVACIDAD = NP.ID_PRIVACIDAD GROUP BY INA.ID_INDICADOR, IND.NOMBRE, INA.PROCESO, INA.SUBPROCESO, PRO.ETIQUETA, SUB.ETIQUETA, NP.ETIQUETA, ind.habilitado ) WHERE TOTAL > 1 ) <filtroColumna> <order> ) row_ WHERE rownum <= :maxRows ) WHERE rownum_ > :minRows

query_reportes_consultaDistritos = SELECT DISTINCT (s.ID_DISTRITO_LOCAL), \
  dl.CABECERA_DISTRITAL_LOCAL \
FROM GEOGRAFICO.SECCIONES s \
INNER JOIN GEOGRAFICO.DISTRITOS_LOCALES dl \
ON s.ID_DISTRITO_LOCAL   = dl.ID_DISTRITO_LOCAL \
AND s.ID_ESTADO          = dl.ID_ESTADO \
WHERE s.id_estado        = :P_IdEstado \
AND s.ID_MUNICIPIO_LOCAL = :P_IdMunicipioLocal \
ORDER BY 1

query_reportes_consultaDemarcaciones = SELECT r.ID_REGIDURIA, \
  r.NOMBRE_REGIDURIA \
FROM GEOGRAFICO.REGIDURIAS r \
WHERE r.id_estado        = :P_IdEstado \
AND r.ID_MUNICIPIO_LOCAL = :P_IdMunicipioLocal \
ORDER BY 1

query_reportes_inicializaEncabezado_obtenAsociaciones_Distritos = SELECT DISTINCT acv.ID_ASOCIACION, \
  acv.TIPO_ASOCIACION, \
  asoc.SIGLAS, \
  cs.SIGLAS SIGLAS_SUBCOALICION, \
  cs.ID_COALICION, \
  acv.ORDEN \
FROM COMPUTOSINE.ACTA_CASILLA_VOTOS acv \
LEFT JOIN ORGANOS_DIRECTIVOS.ASOCIACIONES asoc \
ON acv.ID_ASOCIACION    = asoc.ID_ASOCIACION \
AND acv.ID_ESTADO       = asoc.ID_ESTADO \
AND acv.TIPO_ASOCIACION = asoc.TIPO_ASOCIACION \
LEFT JOIN COMPUTOSINE.C_SUBCOALICIONES cs \
ON acv.ID_ASOCIACION           = cs.ID_SUBCOALICION \
WHERE acv.ID_PROCESO_ELECTORAL = :P_ID_PROCESO_ELECTORAL \
AND acv.ID_DETALLE_PROCESO     = :P_ID_DETALLE_PROCESO \
AND acv.ID_ESTADO              = :P_ID_ESTADO \
AND acv.ID_TIPO_CANDIDATURA    = :P_ID_TIPO_CANDIDATURA \
AND acv.CAPTURADA              = :P_CAPTURADA \
AND acv.ID_DISTRITO           <> -5 \
AND acv.TIPO_ASOCIACION        > 0 \
ORDER BY acv.TIPO_ASOCIACION, \
  cs.ID_COALICION, \
  acv.ORDEN ASC
  
query_reportes_inicializaEncabezado_obtenAsociaciones_Casillas = SELECT DISTINCT acv.ID_ASOCIACION, \
  acv.TIPO_ASOCIACION, \
  asoc.SIGLAS, \
  cs.SIGLAS SIGLAS_SUBCOALICION, \
  cs.ID_COALICION, \
  acv.ORDEN \
FROM COMPUTOSINE.ACTA_CASILLA_VOTOS acv \
LEFT JOIN ORGANOS_DIRECTIVOS.ASOCIACIONES asoc \
ON acv.ID_ASOCIACION    = asoc.ID_ASOCIACION \
AND acv.ID_ESTADO       = asoc.ID_ESTADO \
AND acv.TIPO_ASOCIACION = asoc.TIPO_ASOCIACION \
LEFT JOIN COMPUTOSINE.C_SUBCOALICIONES cs \
ON acv.ID_ASOCIACION           = cs.ID_SUBCOALICION \
WHERE acv.ID_PROCESO_ELECTORAL = :P_ID_PROCESO_ELECTORAL \
AND acv.ID_DETALLE_PROCESO     = :P_ID_DETALLE_PROCESO \
AND acv.ID_ESTADO              = :P_ID_ESTADO \
AND acv.ID_MUNICIPIO           = :P_ID_MUNICIPIO \
AND acv.ID_TIPO_CANDIDATURA    = :P_ID_TIPO_CANDIDATURA \
AND acv.CAPTURADA              = :P_CAPTURADA \
AND acv.TIPO_ASOCIACION        > 0

query_reportes_inicializaEncabezado_obtenAsociaciones_Regidurias = SELECT DISTINCT dp.ID_ASOCIACION, \
  dp.TIPO_ASOCIACION, \
  asoc.SIGLAS, \
  dp.EMBLEMA, \
  dp.ORDEN \
FROM COMPUTOSINE.DISTRIBUCION_PARTIDOS_CI dp \
LEFT JOIN ORGANOS_DIRECTIVOS.ASOCIACIONES asoc \
ON dp.ID_ASOCIACION           = asoc.ID_ASOCIACION \
AND dp.ID_ESTADO              = asoc.ID_ESTADO \
AND dp.TIPO_ASOCIACION        = asoc.TIPO_ASOCIACION \
WHERE dp.ID_PROCESO_ELECTORAL = :P_ID_PROCESO_ELECTORAL \
AND dp.ID_DETALLE_PROCESO     =:P_ID_DETALLE_PROCESO \
AND dp.ID_ESTADO              =:P_ID_ESTADO \
AND dp.ID_MUNICIPIO           =:P_ID_MUNICIPIO \
AND dp.ID_REGIDURIA           =:P_ID_REGIDURIA \
AND dp.ID_TIPO_CANDIDATURA   IN (15,16) \
AND dp.TIPO_ASOCIACION        > 0 \
ORDER BY dp.TIPO_ASOCIACION, \
  dp.ORDEN ASC

query_reportes_inicializaEncabezado_obtenNombreCI = SELECT P.NOMBRE \
  ||' ' \
  || P.APELLIDO_PATERNO \
  ||' ' \
  || P.APELLIDO_MATERNO NOMBRE_COMPLETO \
FROM SNR.REGISTRO R \
JOIN SNR.C_PERSONAS P \
ON R.ID_PERSONA = P.ID_PERSONA \
JOIN SNR.CONFIGURACION_CANDIDATURAS CONF \
ON R.ID_CONFIGURACION_CANDIDATURA = CONF.ID_CONFIGURACION_CANDIDATURA \
WHERE R.CARGO_CANDIDATO           = 1 \
AND R.ID_TIPO_PROCESO             = 2 \
AND R.ESTATUS                     = 2 \
AND R.TIPO_ASOCIACION             = 4 \
AND R.ID_ASOCIACION_CIVIL         = :P_ID_ASOCIACION \
AND R.ID_ESTADO_ELECCION          = :P_ID_ESTADO

query_reportes_valida_existe_casillas_especiales_regidurias = SELECT dp.ID_ASOCIACION \
FROM COMPUTOSINE.ACTA_CASILLA_VOTOS dp \
WHERE dp.ID_PROCESO_ELECTORAL = :P_ID_PROCESO_ELECTORAL \

    /**
     * Formato para nombre y apellidos del aspirante
     */
    private static final String REGEX_FORMATO_NOMBRES = "[a-zA-ZáéíóúÁÉÍÓÚüÜñÑ\\°\\.\\-\\/\\s]*";
    /**
     * Formato RFC
     */
    private static final String REGEX_FORMATO_RFC = "^[A-Z]{4}[0-9]{6}([A-Z0-9]{3})*$";
    /**
     * Formato CURP
     */
    private static final String REGEX_FORMATO_CURP = "^[A-Z]{4}[0-9]{6}[H|M][A-Z]{5}[0-9]{2}$";
    /**
     * Formato de correo electrónico
     */
    private static final String REGEX_FORMATO_CORREO = "^[_A-Za-z0-9-\\+]+(\\.[_A-Za-z0-9-]+)*@[A-Za-z0-9-]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{2,})$";
    /**
     * Formato de correo electrónico de gmail.com
     */
    private static final String REGEX_FORMATO_CORREO_GMAIL = "^[_A-Za-z0-9-\\+]+(\\.[_A-Za-z0-9-]+)*@gmail.com";

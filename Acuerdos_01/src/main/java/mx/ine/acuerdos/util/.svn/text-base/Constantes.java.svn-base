    /**
     * Identificadores que indican si la captura de Ares será 0- por el entorno que tenga el
     * usuario 1- por distrito 2- por municipio
     */
    public enum TIPO_CAPTURA_SECCIONES_ARE {
        CAPTURA_GEOGRAFICO_USUARIO(1), CAPTURA_GEOGRAFICO_DISTRITO(2), CAPTURA_GEOGRAFICO_MUNICIPIO(
            3);

        private final Integer value;

        private TIPO_CAPTURA_SECCIONES_ARE(Integer value) {
            this.value = value;
        }

        public Integer getValue() {
            return this.value;
        }

        public static TIPO_CAPTURA_SECCIONES_ARE getEnum(Integer value) {
            for (TIPO_CAPTURA_SECCIONES_ARE tipo : TIPO_CAPTURA_SECCIONES_ARE.values()) {
                if (value.equals(tipo.value)) {
                    return tipo;
                }
            }
            return null;
        }
    }

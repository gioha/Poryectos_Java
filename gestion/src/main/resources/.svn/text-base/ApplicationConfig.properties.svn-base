#---------------------------------------------------------------------------------
#                              CONFIGURACIONES
#
# Contiene las constantes de las configuraciones del sistema que se este montando 							
#
#---------------------------------------------------------------------------------


################################################################################
# Configuracion general
################################################################################
application.host 	   				= java:/util/urlApplication
application.name 					= directorioDis
application.id						= 100
ldap.url		 					= java:/util/urlCentral
application.captcha  				= true

################################################################################
# Datos del datasource de Base de Datos del servidor
################################################################################
dataSource.jndiName					= java:jboss/gestion4jndi

################################################################################
# Datos del web service
################################################################################
jndi.rest.ws						= java:/util/urlWS
ws.server.name 						= util/urlWS
jndi.certPass.ws					= java:/util/ineCertPassword
################################################################################
# Datos de Hibernate
################################################################################
hibernate.dialect					= org.hibernate.dialect.Oracle10gDialect
hibernate.show_sql					= false
hibernate.format_sql				= false
hibernate.jdbc.batch_size			= 20
hibernate.pack.scan					= mx.ine.gestion.dto
hibernate.pack.scan.servicios		= mx.org.ine.servicios.dto

################################################################################
# Configuracion ws procesos electorales-geografico
################################################################################
comunicaciones.url.gestion_procesos			= gestion-ws/ws/detallesProcesosEstado
comunicaciones.url.gestion_estados			= gestion-ws/ws/detallesEstadosSistema
application.id                   						= 153

################################################################################
# Datos del servidor de SSign
################################################################################
ruta.SeguriSign 					= java:/util/urlSeguriSignWS

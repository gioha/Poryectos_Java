<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui"
	xmlns:pe="http://primefaces.org/ui/material"
	xmlns:ui="http://java.sun.com/jsf/facelets" lang="es-MX"
	xml:lang="es-MX">

<f:view contentType="text/html" encoding="UTF-8" locale="es_MX">

	<h:head>
		<title>Gestión</title>
		
		<link href="http://www.ine.mx/archivos2/portal/img/faviconINE.ico" rel="shortcut icon" />

		<!-- Font -->
		<link href="https://fonts.googleapis.com/css?family=Roboto:400,900" rel="stylesheet"/>

		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />

		<!-- CSS -->
		<h:outputStylesheet name="bootstrap.css" library="css" />
		<h:outputStylesheet name="estilos.css" library="css" />
		<h:outputStylesheet name="font-awesome.css" library="css" />
		<h:outputStylesheet name="global.css" library="css" />
		<h:outputStylesheet name="footer.css" library="css" />
		<h:outputStylesheet name="modulos/menu/menu.css" library="css" />
		<h:outputStylesheet name="animate.css" library="css" />
		<h:outputStylesheet name="yamm.css" library="css"/>
		<h:outputStylesheet name="general_gestion.css" library="css"/>
		<h:outputStylesheet name="estructura.css" library="css" />

		<!-- meta para que en moviles se aplique el modo responsivo y se cancele el zoom-->
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-sca,le=1.0, minimum-scale=1.0, user-scalable=no" />

		<ui:insert name="headIncludes" />

	</h:head>

	<h:body>

		<!--   HEADER   -->
		<ui:include src="header.xhtml" />

		<!-- Colocamos main para que el menú pueda recorrer todo el formulario -->
		<div id="main" >
			<ui:insert name="CONTENT" />
		</div>
		<!-- FOOTER -->
		<ui:include src="footer.xhtml" />

		<!-- JS -->
		<h:outputScript library="primefaces" name="jquery/jquery.js" target="head" />
		<h:outputScript library="primefaces" name="jquery/jquery-plugins.js" />
		<h:outputScript library="js" name="bootstrap.min.js" />
		<h:outputScript library="js" name="primeFacesLocales.js" />
		<h:outputScript library="js" name="footer.js" />
		<h:outputScript library="js" name="menu.js" />


	</h:body>
</f:view>
</html>
package mx.ine.gestion.dto.helper;

import java.io.Serializable;

/**
 * Objeto de transferencia que sirve de ayuda para pintar los meses que se utilizan en los combos que componen los campos de fecha.
 * 
 * @author Roberto Shirásago Domínguez
 * @since 06/09/2017
 */
public class DTOMesesHelper implements Serializable {

	/**
	 * Elemento necesario para la serialización de los objetos generados de esta clase.
	 */
	private static final long serialVersionUID = -1070720406496392805L;

	/* ------------------------------------- Atributos/Campos ------------------------------------ */

	/**
	 * Contiene el nombre del mes (Enero, Febrero, Marzo, etc)
	 */
	private String nombreMes;
	
	/**
	 * Contiene el número del mes
	 */
	private Integer numeroMes;

	
	/* ------------------------------------- Getters y Setters ------------------------------------ */
	
	/**
	 * @return valor de tipo String que esta contenido en la variable nombreMes
	 *
	 * @author Roberto Shirásago Domínguez
	 * @since 04/01/2018
	 */
	public String getNombreMes() {
		return nombreMes;
	}

	/**
	 * @param nombreMes : valor que se ingresa a la variable de tipo String
	 *
	 * @author Roberto Shirásago Domínguez
	 * @since 04/01/2018
	 */
	public void setNombreMes(String nombreMes) {
		this.nombreMes = nombreMes;
	}

	/**
	 * @return valor de tipo Integer que esta contenido en la variable numeroMes
	 *
	 * @author Roberto Shirásago Domínguez
	 * @since 04/01/2018
	 */
	public Integer getNumeroMes() {
		return numeroMes;
	}

	/**
	 * @param numeroMes : valor que se ingresa a la variable de tipo Integer
	 *
	 * @author Roberto Shirásago Domínguez
	 * @since 04/01/2018
	 */
	public void setNumeroMes(Integer numeroMes) {
		this.numeroMes = numeroMes;
	}
	
}
